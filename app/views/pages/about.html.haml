                    .dropdown-menu.dropdown-menu-right
                      %h6.dropdown-header Dropdown header
                      %a.dropdown-item{:href => "javascript:;"} Action
                      %a.dropdown-item{:href => "javascript:;"} Another action
                      %a.dropdown-item{:href => "javascript:;"} Something else here
                      .dropdown-divider
                      %a.dropdown-item{:href => "javascript:;"} Separated link
                      .dropdown-divider
                      %a.dropdown-item{:href => "javascript:;"} One more separated link
      / end menu
      .title
        %h3 Navigation



