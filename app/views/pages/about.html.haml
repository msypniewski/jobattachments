= render "shared/header"
.main.main-raised
  .section.section-basic
    .container
      .title
        %h2 Basic Elements
      / buttons
      #buttons.cd-section
        .title
          %h3
            Buttons
            %br/
            %small Pick your style
        .row
          .col-md-8.mx-auto
            %button.btn.btn-primary Default
            %button.btn.btn-primary.btn-round Round
            %button.btn.btn-primary.btn-round
              %i.material-icons favorite
              With Icon
            %button.btn.btn-primary.btn-fab.btn-round
              %i.material-icons favorite
            %button.btn.btn-primary.btn-link Simple
        .title
          %h3
            %small Pick your size
        .row
          .col-md-8.mx-auto
            %button.btn.btn-primary.btn-sm Small
            %button.btn.btn-primary Regular
            %button.btn.btn-primary.btn-lg Large
        .title
          %h3
            %small Pick your color
        .row
          .col-md-10.ml-auto
            %button.btn Default
            %button.btn.btn-primary Primary
            %button.btn.btn-info Info
            %button.btn.btn-success Success
            %button.btn.btn-warning Warning
            %button.btn.btn-danger Danger
            %button.btn.btn-rose Rose
      / end buttons
      .space-50
      / inputs
      #inputs
        .title
          %h3 Inputs
        .row
          .col-lg-3.col-sm-4
            .form-group.has-default
              %input.form-control{:placeholder => "Regular", :type => "text"}/
          .col-lg-3.col-sm-4
            .form-group
              %label.bmd-label-floating{:for => "exampleInput1"} With Floating Label
              %input#exampleInput1.form-control{:type => "email"}/
              %span.bmd-help We'll never share your email with anyone else.
          .col-lg-3.col-sm-4
            .form-group.has-success
              %label.bmd-label-floating{:for => "exampleInput2"} Success input
              %input#exampleInput2.form-control{:type => "text"}/
              %span.form-control-feedback
                %i.material-icons done
          .col-lg-3.col-sm-4
            .form-group.has-danger
              %label.bmd-label-floating{:for => "exampleInput3"} Error input
              %input#exampleInput3.form-control{:type => "email"}/
              %span.form-control-feedback
                %i.material-icons clear
          .col-lg-3.col-sm-4
            .form-group
              .input-group
                .input-group-prepend
                  %span.input-group-text
                    %i.material-icons group
                %input.form-control{:placeholder => "With Material Icons", :type => "text"}/
          .col-lg-3.col-sm-4
            .form-group
              .input-group
                .input-group-prepend
                  %span.input-group-text
                    %i.fa.fa-group
                %input.form-control{:placeholder => "With Font Awesome Icons", :type => "text"}/
      / end inputs
      .space-70
      / textarea/checkbox/radio/toggle
      #checkRadios
        .row
          .col-lg-3.col-md-4.col-sm-6
            .title
              %h3 Checkboxes
            .form-check
              %label.form-check-label
                %input.form-check-input{:type => "checkbox", :value => ""}/
                Unchecked
                %span.form-check-sign
                  %span.check
            .form-check
              %label.form-check-label
                %input.form-check-input{:checked => "checked", :type => "checkbox", :value => ""}/
                Checked
                %span.form-check-sign
                  %span.check
            .form-check.disabled
              %label.form-check-label
                %input.form-check-input{:disabled => "disabled", :type => "checkbox", :value => ""}/
                Disabled unchecked
                %span.form-check-sign
                  %span.check
            .form-check.disabled
              %label.form-check-label
                %input.form-check-input{:checked => "checked", :disabled => "disabled", :type => "checkbox", :value => ""}/
                Disabled checked
                %span.form-check-sign
                  %span.check
          .col-lg-3.col-md-4.col-sm-6
            .title
              %h3 Radio Buttons
            .form-check
              %label.form-check-label
                %input#exampleRadios1.form-check-input{:name => "exampleRadios", :type => "radio", :value => "option1"}/
                Radio is off
                %span.circle
                  %span.check
            .form-check
              %label.form-check-label
                %input#exampleRadios2.form-check-input{:checked => "checked", :name => "exampleRadios", :type => "radio", :value => "option2"}/
                Radio is on
                %span.circle
                  %span.check
            .form-check.disabled
              %label.form-check-label
                %input#exampleRadios11.form-check-input{:disabled => "disabled", :name => "exampleRadios1", :type => "radio", :value => "option1"}/
                Disabled radio is off
                %span.circle
                  %span.check
            .form-check.disabled
              %label.form-check-label
                %input#exampleRadios21.form-check-input{:checked => "checked", :disabled => "disabled", :name => "exampleRadio1", :type => "radio", :value => "option2"}/
                Disabled radio is on
                %span.circle
                  %span.check
          .col-lg-3.col-md-4.col-sm-6
            .title
              %h3 Toggle Buttons
            .togglebutton
              %label
                %input{:checked => "checked", :type => "checkbox"}/
                %span.toggle
                Toggle is on
            .togglebutton
              %label
                %input{:type => "checkbox"}/
                %span.toggle
                Toggle is off
      .space-70
      / progress/sliders
      #progress
        .row
          .col-md-6
            .title
              %h3 Progress Bars
            .progress.progress-line-primary
              .progress-bar.progress-bar-primary{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", :style => "width: 30%;"}
                %span.sr-only 60% Complete
            .progress.progress-line-info
              .progress-bar.progress-bar-info{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", :style => "width: 60%;"}
                %span.sr-only 60% Complete
            .progress.progress-line-danger
              .progress-bar.progress-bar-success{:style => "width: 35%"}
                %span.sr-only 35% Complete (success)
              .progress-bar.progress-bar-warning{:style => "width: 20%"}
                %span.sr-only 20% Complete (warning)
              .progress-bar.progress-bar-danger{:style => "width: 10%"}
                %span.sr-only 10% Complete (danger)
          .col-md-6
            .title
              %h3 Pagination
            %ul.pagination.pagination-primary
              /
                color-classes: "pagination-primary", "pagination-info", "pagination-success", "pagination-warning", "pagination-danger"
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 1
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} ...
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 5
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 6
              %li.active.page-item
                %a.page-link{:href => "javascript:void(0);"} 7
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 8
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 9
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} ...
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 12
            %ul.pagination.pagination-info
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} prev
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 1
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 2
              %li.active.page-item
                %a.page-link{:href => "javascript:void(0);"} 3
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 4
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} 5
              %li.page-item
                %a.page-link{:href => "javascript:void(0);"} next
      / end progress
      / sliders
      #sliders
        .row
          .col-md-6
            .title
              %h3 Sliders
            #sliderRegular.slider
            #sliderDouble.slider.slider-info
          .col-md-6.ml-auto
            .title
              %h3 Badges
            %span.badge.badge-pill.badge-secondary Default
            %span.badge.badge-pill.badge-primary Primary
            %span.badge.badge-pill.badge-info Info
            %span.badge.badge-pill.badge-success Success
            %span.badge.badge-pill.badge-warning Warning
            %span.badge.badge-pill.badge-danger Danger
            %span.badge.badge-pill.badge-rose Rose
      / end sliders
