/ navbar
#navbar
  .navigation-example
    / rose navbar with search form
    %nav.navbar.navbar-expand-lg.bg-rose
      .container
        .navbar-translate
          %a.navbar-brand{:href => "#0"} Brand
          %button.navbar-toggler{"aria-expanded" => "false", "aria-label" => "Toggle navigation", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.navbar-toggler-icon
            %span.navbar-toggler-icon
            %span.navbar-toggler-icon
        .collapse.navbar-collapse
          %ul.navbar-nav
            %li.nav-item.active
              %a.nav-link{:href => "javascript:;"} link
            %li.nav-item
              %a.nav-link{:href => "javascript:;"} link
          %form.form-inline.ml-auto
            .form-group.has-white
              %input.form-control{:placeholder => "Search", :type => "text"}/
            %button.btn.btn-white.btn-raised.btn-fab.btn-round{:type => "submit"}
              %i.material-icons search
    / end rose navbar
