    / inverse navbar with notifications
    %nav.navbar.navbar-inverse.navbar-expand-lg.bg-dark{:role => "navigation-demo"}
      .container
        / Brand and toggle get grouped for better mobile display
        .navbar-translate
          %a.navbar-brand{:href => "#0"} Navbar with notification
          %button.navbar-toggler{"aria-expanded" => "false", "aria-label" => "Toggle navigation", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.navbar-toggler-icon
            %span.navbar-toggler-icon
            %span.navbar-toggler-icon
        / Collect the nav links, forms, and other content for toggling
        .collapse.navbar-collapse
          %ul.navbar-nav.ml-auto
            %li.nav-item
              %a.nav-link{:href => "javascript:;"}
                Discover
            %li.nav-item
              %a.nav-link{:href => "javascript:;"}
                Wishlist
            %li.nav-item
              %a.btn.btn-rose.btn-raised.btn-fab.btn-round{"data-toggle" => "dropdown", :href => "javascript:;"}
                %i.material-icons email
            %li.dropdown.nav-item
              %a.profile-photo.dropdown-toggle.nav-link{"data-toggle" => "dropdown", :href => "javascript:;"}
                .profile-photo-small
                  %img.rounded-circle.img-fluid{:alt => "Circle Image", :src => "#{asset_pack_path 'avatar.jpg'}"}/
              .dropdown-menu.dropdown-menu-right
                %h6.dropdown-header Dropdown header
                %a.dropdown-item{:href => "javascript:;"} Me
                %a.dropdown-item{:href => "javascript:;"} Settings and other stuff
                %a.dropdown-item{:href => "javascript:;"} Sign out
        / /.navbar-collapse
      / /.container
    / end inverse navbar
